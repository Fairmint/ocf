{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://opencaptablecoalition.com/schema/files/valuations_file",
  "title": "File - Valuations",
  "description": "List of valuations for the issuer (meant for use in validating valuations loaded into an array from JSON or JSONL)",
  "type": "object",
  "allOf": [{ "$ref": "/schema/primitives/base_file" }],
  "properties": {
    "items": {
      "type": "array",
      "items": {
        "$ref": "/schema/objects/valuation"
      }
    },
    "file_type": {
      "const": "OCF_VALUATIONS_FILE"
    }
  },
  "required": ["items", "file_type"]
}
